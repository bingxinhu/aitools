AA,GAAA,CAAA;MACA;IACA,CAAA;IACA,QAAA,EAAA;MACA,GAAA,iBAAA;QACA,OAAA,IAAA,CAAA,KAAA;MACA,CAAA;MACA,GAAA,eAAA,GAAA,EAAA;QACA,IAAA,CAAA,KAAA,CAAA,cAAA,EAAA,GAAA,CAAA;MACA;IACA;EACA,CAAA;EACA,OAAA,EAAA;IACA,gBAAA,4BAAA,GAAA,EAAA;MACA,IAAA,CAAA,KAAA,CAAA,YAAA,EAAA;QAAA,IAAA,EAAA,IAAA,CAAA,WAAA;QAAA,KAAA,EAAA;MAAA,CAAA,CAAA;MACA,IAAA,IAAA,CAAA,UAAA,EAAA;QACA,QAAA,CAAA,CAAA,EAAA,GAAA,CAAA;MACA;IACA,CAAA;IACA,mBAAA,+BAAA,GAAA,EAAA;MACA,IAAA,CAAA,KAAA,CAAA,YAAA,EAAA;QAAA,IAAA,EAAA,GAAA;QAAA,KAAA,EAAA,IAAA,CAAA;MAAA,CAAA,CAAA;MACA,IAAA,IAAA,CAAA,UAAA,EAAA;QACA,QAAA,CAAA,CAAA,EAAA,GAAA,CAAA;MACA;IACA;EACA;AACA,CAAA","sourcesContent":["<template>\n  <div :class=\"{'hidden':hidden}\" class=\"pagination-container\">\n    <el-pagination\n      :background=\"background\"\n      :current-page.sync=\"currentPage\"\n      :page-size.sync=\"pageSize\"\n      :layout=\"layout\"\n      :page-sizes=\"pageSizes\"\n      :total=\"total\"\n      v-bind=\"$attrs\"\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n    />\n  </div>\n</template>\n\n<script>\nimport { scrollTo } from '@/utils/scroll-to'\n\nexport default {\n  name: 'Pagination',\n  props: {\n    total: {\n      required: true,\n      type: Number\n    },\n    page: {\n      type: Number,\n      default: 1\n    },\n    limit: {\n      type: Number,\n      default: 20\n    },\n    pageSizes: {\n      type: Array,\n      default() {\n        return [10, 20, 30, 50]\n      }\n    },\n    layout: {\n      type: String,\n      default: 'total, sizes, prev, pager, next, jumper'\n    },\n    background: {\n      type: Boolean,\n      default: true\n    },\n    autoScroll: {\n      type: Boolean,\n      default: true\n    },\n    hidden: {\n      type: Boolean,\n      default: false\n    }\n  },\n  computed: {\n    currentPage: {\n      get() {\n        return this.page\n      },\n      set(val) {\n        this.$emit('update:page', val)\n      }\n    },\n    pageSize: {\n      get() {\n        return this.limit\n      },\n      set(val) {\n        this.$emit('update:limit', val)\n      }\n    }\n  },\n  methods: {\n    handleSizeChange(val) {\n      this.$emit('pagination', { page: this.currentPage, limit: val })\n      if (this.autoScroll) {\n        scrollTo(0, 800)\n      }\n    },\n    handleCurrentChange(val) {\n      this.$emit('pagination', { page: val, limit: this.pageSize })\n      if (this.autoScroll) {\n        scrollTo(0, 800)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.pagination-container {\n  background: #fff;\n  padding: 32px 16px;\n}\n.pagination-container.hidden {\n  display: none;\n}\n</style>\n"],"sourceRoot":"src/components/Pagination"}]}